	   <mapping resource="bs/photoAdmin/model/PhotoInfo.hbm.xml" />
	   <mapping resource="bs/photoAdmin/model/UserHasAlbumInfo.hbm.xml" />
	   <mapping resource="bs/photoAdmin/model/UsericonInfo.hbm.xml" />
	   <mapping resource="bs/photoAdmin/model/UserInfo.hbm.xml" />
	   <mapping resource="bs/photoAdmin/model/TagInfo.hbm.xml" />
